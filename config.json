{
    "log_file": "app.log",
    "base_dir": "./DTA",
    "serial_ports": [
        {
            "port": "/dev/ttyUSB0",
            "baudrate": 9600,
            "bytesize": 8,
            "parity": "N",
            "stopbits": 1,
            "timeout": 1
        },
        {
            "port": "/dev/ttyUSB1",
            "baudrate": 9600,
            "bytesize": 8,
            "parity": "N",
            "stopbits": 1,
            "timeout": 1
        },
        {
            "port": "/dev/ttyUSB2",
            "baudrate": 9600,
            "bytesize": 8,
            "parity": "N",
            "stopbits": 1,
            "timeout": 1
        },
        {
            "port": "/dev/ttyUSB3",
            "baudrate": 9600,
            "bytesize": 8,
            "parity": "N",
            "stopbits": 1,
            "timeout": 1
        },
        {
            "port": "/dev/ttyUSB4",
            "baudrate": 9600,
            "bytesize": 8,
            "parity": "N",
            "stopbits": 1,
            "timeout": 1
        }
    ],
    "zabbix_keys": {
        "inclinometer": {
            "radial": "tilt.radial",
            "tangential": "tilt.tangential",
            "temperature": "tilt.temp",
            "voltage": "tilt.vbat"
        },
        "pluviometer": {
            "rain_level": "rain.level",
            "voltage": "rain.vbat"
        }
    }
